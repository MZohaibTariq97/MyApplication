<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <gradient
                android:angle="135"
                android:endColor="#FFFFB980"
                android:centerColor="#FFBC65BD"
                android:startColor="#FF3C3DFF" />
            <size
                android:width="56dp"
                android:height="56dp" />
        </shape>
    </item>
    <item android:gravity="center"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp"
        android:bottom="5dp">
        <bitmap android:src="@drawable/icons8_alarm_clock_24"
            android:gravity="center" />

    </item>
</layer-list>